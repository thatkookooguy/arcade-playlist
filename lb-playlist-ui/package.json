{
  "name": "lb-playlist-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href /arcade-playlist/ --aot --extract-licenses --optimization",
    "watch": "ng build --watch --configuration development",
