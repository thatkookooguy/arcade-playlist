    "@types/lodash": "^4.14.199",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.8.9",
    "chance": "^1.1.11",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
